section:Button {
    Name = "Skip Chat (Slightly Bugged) ⏭️",
    Description = nil,
    Callback = function()
        local SkipChat = true
        local OldWait
        OldWait =
            hookfunction(
            _p.NPCChat.AdvanceSignal:wait(),
            function(...)
                if SkipChat then
                    return
                end
                OldWait(...)
            end
        )
        local Old
        Old = hookfunction(
            _p.NPCChat:say(),
            function(...)
                local Args = {...}
                if SkipChat then
                    for i = 3, #Args do
                        if typeof(Args[i]) == "string" then
                            Args[i] = "."
                        end
                    end
                end
                return Old(unpack(Args))
        end)
    end
}
